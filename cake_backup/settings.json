{
	"version": {
		"VersionFile": "./version.json",
		"AssemblyInfoFile": "./src/StravaSharp/Properties/AssemblyInfo.cs",
		"LoadFrom": "VersionFile",
		"AutoIncrementVersion": false,
		"NamespaceBase": "StravaSharp"
	},
	"build": {
		"SourcePath": "./src",
		"SolutionFileSpec": "./src/**/*.sln",
		"TreatWarningsAsErrors": false,
		"NugetConfigPath": "./.nuget/NuGet.Config",
	},
	"nuget": {
		"NuGetConfig": "./src/.nuget/NuGet.config",
		"FeedUrl": "https://www.nuget.org",
		"FeedAPIKey": "",
		"ArtifactsPath": "./artifacts/packages",
		"NuspecPath": "./nuspec",
		"UpdateVersion": true,
		"VersionDependencyForLibrary": "exact"
	}
}
